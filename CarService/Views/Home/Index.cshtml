               <div id="commentContainer" class="container">
                    @addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
                    @model IEnumerable<CarService.Models.Comments>
                    @{ foreach (var item in Model)
                        {
                            <br />
                            <div class="card card-inner">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-md-2">
                                            <p class="text-secondary text-center">@item.Date.ToString("dd/MM/yyyy") </p>
                                        </div>
                                        <div class="col-md-10">
                                            <p>
                                                <strong>
                                                    @{List<CarService.Models.Account> names = ViewBag.names; }
                                                    @{var name = names.SingleOrDefault(p => p.IdUser == item.IdUser).Name; }
                                                    <span>@name</span>
                                                </strong>
                                                @{var score = item.Score; }
                                                @for (int i = 0; i < score; i++)
                                                {
                                                    <span class="float-right"><i class="text-warning fa fa-star"></i></span>
                                                }
                                            </p>

                                            <p>@item.Comment</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                    }
                </div>
            </div>
